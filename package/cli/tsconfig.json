{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "strict": true,
    "outDir": "dist",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "esModuleInterop": true
  }
}
